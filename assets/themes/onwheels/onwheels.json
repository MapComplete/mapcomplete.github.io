          }
        ]
      }
    },
    {
      "builtin": "entrance",
      "override": {
        "minzoom": 19,
        "mapRendering": [
          {
            "icon": "circle:white;./assets/themes/onwheels/entrance.svg",
            "iconSize": "40,40,center"
          }
        ],
        "syncSelection": "theme-only",
        "filter": [
          {
            "id": "width",
            "options": [
