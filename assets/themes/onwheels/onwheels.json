    "walls_and_buildings",
    {
      "builtin": "elevator",
      "override": {
        "minzoom": 19,
        "syncSelection": "theme-only",
        "mapRendering": [
          {
            "icon": "circle:white;./assets/themes/onwheels/elevator.svg",
            "iconSize": "40,40,bottom"
          }
